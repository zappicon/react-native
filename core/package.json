{
  "name": "@zappicon/core",
  "version": "1.0.0",
  "description": "Core icon data for Zappicon packages",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf src/index.ts",
    "convert": "pnpm run clean && node scripts/convert.mjs",
    "build": "pnpm run convert",
    "commitlint": "commitlint --edit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "zappicon"
  ],
  "author": {
    "name": "Haroun Abidi",
    "email": "contact@harounabidi.com",
    "url": "https://harounabidi.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "svgo": "^4.0.0",
    "svgson": "^5.3.1",
    "ts-jest": "^29.4.1",
    "typescript-eslint": "^8.39.0"
  }
}
