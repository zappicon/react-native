{
  "name": "@zappicon/react-native",
  "version": "1.2.1",
  "description": "Free & premium UI icons library, crafted to simplify workflows for designers, developers and creators.",
  "main": "dist/index.cjs.js",
  "browser": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./icons/*": {
      "import": "./dist/icons/*.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "icons",
    "react-native",
    "svg",
    "components",
    "zappicon"
  ],
  "author": "Haroun Abidi (Developed under Zappicon) <contact@harounabidi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappicon/react-native.git"
  },
  "bugs": {
    "url": "https://github.com/zappicon/react-native/issues"
  },
  "homepage": "https://zappicon.com",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist && rimraf src/icons && rimraf src/variants && rimraf src/index.ts",
    "generate": "npm run clean && tsx scripts/generate.ts",
    "build": "npm run generate && vite build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.1",
    "@react-native/babel-preset": "^0.82.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^11.1.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jiti": "^2.6.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "^0.81.5",
    "react-native-svg": "^15.15.0",
    "react-test-renderer": "19.2.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
